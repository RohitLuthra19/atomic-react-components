{
  "presets": ["react", "flow"],
  "plugins": [
    ["@quickbaseoss/babel-plugin-styled-components-css-namespace", { "cssNamespace": "lib" }],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": false
      }
    ],
    ["transform-object-rest-spread"]
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    },
    "test": {
      "presets": ["es2015", "react", "stage-0"]
    }
  }
}
