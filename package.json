{
  "name": "atomic-react-pattern-lib",
  "version": "0.1.0",
  "description": "An React Pattern Library",
  "main": "dist/index.js",
  "scripts": {
    "flow": "flow",
    "flow:coverage": "flow-coverage-report -i 'lib/**/*.js' -x 'lib/**/*.test.js' -x 'lib/**/*.style.js' -t html -t json -t text -o reports/flow/ --threshold 70",
    "build": "babel lib -d dist",
    "lint:js": "eslint lib/**; exit 0",
    "lint:css": "stylelint lib/**",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:watch": "esw -w lib/**",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006 -s ./lib/static",
    "deploy": "build-storybook -c .storybook -o .out",
    "test": "cross-env NODE_ENV=test jest --config ./config/jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --config ./config/jest.config.js",
    "audit": "npm run build -- --report",
    "build-storybook": "build-storybook",
    "generate": "plop --plopfile generators/index.js"
  },
  "keywords": [
    "Atomic",
    "React",
    "Pattern Lib"
  ],
  "author": "Sapient XT",
  "license": "ISC",
  "devDependencies": {
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^0.1.1",
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-console": "^1.0.4",
    "@storybook/addon-viewport": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "autoprefixer": "^9.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-redux-saga": "^0.9.0",
    "eslint-watch": "^4.0.2",
    "flow-bin": "^0.81.0",
    "flow-coverage-report": "^0.6.0",
    "flow-typed": "^2.5.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-styled-components": "^6.2.0",
    "plop": "^2.1.0",
    "react-storybook-addon-chapters": "^2.1.7",
    "react-test-renderer": "^16.5.2",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.4.9",
    "flexboxgrid": "^6.3.1",
    "normalize.css": "^8.0.0",
    "next": "^5.1.0"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "jestSonar": {
    "reportPath": "reports/tests",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "resolutions": {
    "eslint-scope": "3.7.1"
  }
}
